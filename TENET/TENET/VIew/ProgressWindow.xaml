<reactiveUi:ReactiveWindow
        x:TypeArguments="local:ProgressViewModel"
        x:Class="TENET.ProgressWindow"
        xmlns:reactiveUi="http://reactiveui.net"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TENET" 
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d"
        Title="Прогресс проекта" Height="470" Width="880" WindowStartupLocation="CenterScreen">

    <Window.Background>
        <ImageBrush ImageSource="/TENET;component/images/fon3.png" x:Name="im"/>
    </Window.Background>

    <Grid Margin="33,21,32,30">

        <Button Click="button_Click" x:Name="ButtonBack" Width="150" Height="50" Margin="0,298,0,0">Назад</Button>
        <DataGrid Height="192" Margin="0,40,0,0" VerticalAlignment="Top" AutoGenerateColumns="True" x:Name="ProgressGrid" IsReadOnly="true"/>
    </Grid>

</reactiveUi:ReactiveWindow>
